; Definition file for Kung Fu Man
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Frank West Arr"        ;Name of character
displayname = "Frank West" ;Name of character to display
versiondate = 9/27/14    ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.0          ;Version of M.U.G.E.N character works on (X.XX)
author = "Kamekaze"         ;Character author name
pal.defaults = 1,2,3,4      ;Default palettes in order of preference (up to 4)
                            ;Numbering starts from 1
localcoord = 320,240       ;Local coordinate space width and height

; Files for the player
[Files]
sound   = Frank-KM.snd 
cmd     = Frank-KM.cmd           ;Command set
cns     = Frank-KM.cns           ;Constants
st      = Frank-KM.cns           ;States
st1 = custom.cns
st2 = Helper.st
st3 = config.txt
stcommon = common.cns      ;Common states (from data/ or motif)
sprite  = Frank-KM.sff           ;Sprites by 
anim    = Frank-KM.air           ;Animation
          ;Sound
[Palette Keymap]
x = 1 ;Press button X to select palette 1, etc.
y = 2
z = 3
a =4
b = 5
c = 6
x2 = 7 ;Hold Start and press button X for palette 7, etc.
y2 = 8
z2 = 9
a2 = 10
b2 = 11
c2 = 12
; Arcade mode
[Arcade]
intro.storyboard = intro.def
ending.storyboard = ending.def
